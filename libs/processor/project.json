{
  "name": "processor",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/processor/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@monodon/rust:check",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/processor"
      }
    },
    "build-ios": {
      "command": "bash build-ios.sh",
      "outputs": [
        "{projectRoot}/dist/*"
      ],
      "options": {
        "cwd": "{projectRoot}"
      },
      "inputs": [
        "{projectRoot}/src/**/*",
        "{projectRoot}/Cargo.toml",
        "{projectRoot}/build.rs",
        "{projectRoot}/cbindgen.toml",
        "{projectRoot}/build-ios.sh"
      ],
      "cache": true
    },
    "test": {
      "cache": true,
      "executor": "@monodon/rust:test",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/processor"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "cache": true,
      "executor": "@monodon/rust:lint",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/processor"
      }
    }
  }
}