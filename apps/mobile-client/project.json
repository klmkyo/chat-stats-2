{
  "targets": {
    "sync-processor": {
      "command": "bash modules/processor-bridge/ios/scripts/sync-processor.sh",
      "options": {
        "cwd": "{projectRoot}"
      },
      "outputs": [
        "{projectRoot}/modules/processor-bridge/ios/Vendored/Processor"
      ],
      "dependsOn": [
        {
          "target": "build-ios",
          "projects": [
            "processor"
          ],
          "params": "ignore"
        }
      ]
    },
    "prebuild": {
      "dependsOn": [
        "sync-processor"
      ]
    },
    "ios": {
      "dependsOn": [
        "sync-processor"
      ]
    }
  }
}